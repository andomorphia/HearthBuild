{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "brace-style": [
      "error",
      "stroustrup"
    ],
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "no-mixed-operators": [
      "error",
      {
        "allowSamePrecedence": true
      }
    ],
    "no-shadow": "off",
    "no-unused-vars": "off",
    "consistent-return": "off",
    "no-multi-assign": "off",
    "no-undef": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "max-len": 0,
    "jsx-a11y/no-static-element-interactions": "off",
    "jsx-a11y/href-no-hash": "off",
    "react/jsx-filename-extension": "off",
    "react/forbid-prop-types": "off",
    "react/no-unescaped-entities": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "app/"
        ]
      }
    }
  }
}
